                                    android:textSize="15dp"
                                    android:layout_marginLeft="11dp"
                                    android:layout_marginRight="11dp"
                                    android:layout_marginTop="8dp"
                                    android:layout_marginBottom="8dp"
                                    android:text="Bad"/>

