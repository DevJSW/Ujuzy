<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:background="@color/white"
    android:layout_height="match_parent">

    <include layout="@layout/white_toolbar"
        android:id="@+id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        android:background="?attr/colorPrimary" />

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:background="@color/grey_line"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="13dp"
        android:layout_marginRight="13dp"
        android:orientation="vertical">

        
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="60dp">

            <com.ujuzy.ujuzy.ujuzy.CustomData.AllerFont
                android:layout_width="match_parent"
                android:id="@+id/openEditProfile"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="5dp"
                android:textSize="16dp"
                android:textColor="@color/black"
                android:text="Edit Profile"/>

        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/grey_line"/>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="60dp">

            <com.ujuzy.ujuzy.ujuzy.CustomData.AllerFont
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="5dp"
                android:textSize="16dp"
                android:textColor="@color/black"
                android:text="Upgrade"/>

        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/grey_line"/>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="60dp">

            <com.ujuzy.ujuzy.ujuzy.CustomData.AllerFont
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="5dp"
                android:textSize="16dp"
                android:textColor="@color/black"
                android:text="Claim Business"/>

        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/grey_line"/>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="60dp">

            <com.ujuzy.ujuzy.ujuzy.CustomData.AllerFont
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/inviteFriend"
                android:layout_centerVertical="true"
                android:layout_marginLeft="5dp"
                android:textSize="16dp"
                android:textColor="@color/black"
                android:text="Invite a Friend"/>

        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/grey_line"/>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="60dp">

            <com.ujuzy.ujuzy.ujuzy.CustomData.AllerFont
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/legal"
                android:layout_centerVertical="true"
                android:layout_marginLeft="5dp"
                android:textSize="16dp"
                android:textColor="@color/black"
                android:text="Legal"/>

        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/grey_line"/>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="60dp">

            <com.ujuzy.ujuzy.ujuzy.CustomData.AllerFont
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:id="@+id/openCreateService"
                android:layout_marginLeft="5dp"
                android:textSize="16dp"
                android:textColor="@color/black"
                android:text="Create service"/>

        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/grey_line"/>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="60dp">

            <com.ujuzy.ujuzy.ujuzy.CustomData.AllerFont
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/openLogin"
                android:layout_centerVertical="true"
                android:layout_marginLeft="5dp"
                android:textSize="16dp"
                android:textColor="@color/black"
                android:text="Log In"/>

        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/grey_line"/>

    </LinearLayout>

</LinearLayout>
